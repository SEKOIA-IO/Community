{
    "name": "Enrich alerts with hostnames",
    "nodes": {
        "1": {
            "icon": "data:image/svg+xml;base64,",
            "name": "Get Alert",
            "type": "action",
            "outputs": {
                "default": [
                    "5"
                ]
            },
            "arguments": {
                "stix": false,
                "uuid": "{{ node.7.alert_uuid }}"
            },
            "action_uuid": "8d189665-5401-4098-8d60-944de9a6199a",
            "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a"
        },
        "2": {
            "icon": "data:image/svg+xml;base64,",
            "name": "Get Events",
            "type": "action",
            "outputs": {
                "default": [
                    "4"
                ]
            },
            "arguments": {
                "query": "alert_short_ids: {{ node.1.short_id }}",
                "latest_time": "{{ node.1.last_seen_at }}",
                "earliest_time": "{{ node.1.first_seen_at }}"
            },
            "action_uuid": "af0b4355-a428-43d6-991c-d5ff878e17d5",
            "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a"
        },
        "3": {
            "name": "Store",
            "type": "operator",
            "outputs": {
                "default": []
            },
            "subtype": "store",
            "modifications": [
                {
                    "key": "hostnames",
                    "type": "append",
                    "value": "{{ node.4.default.value[\"host.name\"] }}"
                }
            ]
        },
        "4": {
            "loop": [
                "3"
            ],
            "name": "Foreach",
            "type": "operator",
            "items": "{{ node.2.events }}",
            "outputs": {
                "default": [
                    "6"
                ]
            },
            "subtype": "foreach"
        },
        "5": {
            "name": "Condition",
            "type": "operator",
            "cases": [
                {
                    "left": "{{ node.1.urgency['value'] }}",
                    "name": "Check criticity",
                    "right": "{{ 70 }}",
                    "comparison": ">="
                }
            ],
            "outputs": {
                "Check criticity": [
                    "2"
                ],
                "else": []
            },
            "subtype": "condition"
        },
        "6": {
            "icon": "data:image/svg+xml;base64,",
            "name": "Comment Alert",
            "type": "action",
            "outputs": {
                "default": []
            },
            "arguments": {
                "uuid": "{{ node.7.alert_uuid }}",
                "content": "Here you can find all related hostnames \n{{ store.hostnames | unique | join(',') }}"
            },
            "action_uuid": "0d323de3-a864-4afe-a0c3-e7ff45883d7a",
            "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a"
        },
        "7": {
            "icon": "data:image/svg+xml;base64,",
            "name": "Alert Created",
            "type": "trigger",
            "outputs": {
                "default": [
                    "1"
                ]
            },
            "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a",
            "trigger_uuid": "785583cf-4377-43ce-a5fb-f4572eb57456"
        }
    },
    "workspace": "Operation Center",
    "description": "Add a comment on an alert if host.name field exists in related events",
    "uuid": "96a31d46-3a55-4464-a4d2-c2fabf60ac08"
}