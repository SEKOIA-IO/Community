{
    "name": "Automatically create an alert on TheHive",
    "description": "Automatically create an alert on TheHive when a new alert is raised in SEKOIA.IO.",
    "tags": ["alert", "thehive"],
    "workspace": "Operation Center",
    "nodes": {
        "0": {
            "name": "Alert Created",
            "type": "trigger",
            "icon": "data:image/svg+xml;base64,",
            "outputs": {"default": ["1"]},
            "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a",
            "trigger_uuid": "785583cf-4377-43ce-a5fb-f4572eb57456"
        },
        "1": {
            "icon": "data:image/svg+xml;base64,",
            "name": "Get alert",
            "type": "action",
            "outputs": {"default": ["2"]},
            "arguments": {"stix": false, "uuid": "{{ node.0['alert_uuid'] }}"},
            "action_uuid": "8d189665-5401-4098-8d60-944de9a6199a",
            "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a"
        },
        "2": {
            "icon": "data:image/svg+xml;base64,",
            "name": "Get the alert events",
            "type": "action",
            "outputs": {"default": ["3"]},
            "arguments": {
                "query": "alert_short_ids:\"{{ node.1['short_id'] }}\"",
                "latest_time": "now",
                "earliest_time": "-1d"
            },
            "action_uuid": "af0b4355-a428-43d6-991c-d5ff878e17d5",
            "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a"
        },
        "3": {
            "icon": "data:image/png;base64,",
            "name": "Create the alert in TheHive",
            "type": "action",
            "outputs": {"default": []},
            "arguments": {
                "alert": "{{ node.1 }}",
                "events": "{{ node.2['events'] }}",
                "artifact_sighted": false,
                "artifact_ignore_similarity": false
            },
            "action_uuid": "2356ea35-c875-41da-b9f2-6fdbf9e372e6",
            "module_uuid": "48086812-d709-4eff-b308-e8bf504b8a7a"
        }
    }
}
