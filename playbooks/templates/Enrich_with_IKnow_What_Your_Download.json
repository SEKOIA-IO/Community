{
  "name": "Enrich with IKnowWhatYourDownload",
  "nodes": {
    "0": {
      "name": "alert_webhook",
      "type": "trigger",
      "outputs": {
        "default": [
          "4"
        ]
      },
      "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a",
      "trigger_uuid": "fc26eb9f-b272-4c15-b3bf-ace397c0dc57"
    },
    "1": {
      "name": "Read for IP",
      "type": "action",
      "outputs": {
        "default": [
          "2"
        ]
      },
      "arguments": {
        "file": "{{ node.4.stix }}",
        "to_file": false,
        "jsonpath": "$.objects[?(type=\"observed-data\")].objects[?(@.type=\"ipv4-addr\")].value",
        "return_list": true
      },
      "action_uuid": "ecedfa52-7033-4ea8-9c3b-93ad34295f87",
      "module_uuid": "07cce76b-a319-40ee-a0cf-1ba433431e21"
    },
    "2": {
      "name": "IP Check",
      "type": "action",
      "outputs": {
        "known": [
          "3"
        ],
        "unknown": [
          "5"
        ]
      },
      "arguments": {
        "ip": "{{ node.1['output'][0] }}"
      },
      "action_uuid": "f5d6d623-8e3d-4d8e-8843-19058812b8f9",
      "module_uuid": "3c334ccd-91be-49d5-9267-915db6ab588e"
    },
    "3": {
      "icon": "data:image/svg+xml;base64,",
      "name": "post_a_comment_on_an_alert",
      "type": "action",
      "outputs": {
        "default": []
      },
      "arguments": {
        "uuid": "{{ node.0['alert_uuid'] }}",
        "author": "IP Check",
        "content": "This IP `{{ node.1['output'][0] }}` is know for being a BitTorrent node at this date: {{ node.2['date'] }}."
      },
      "action_uuid": "0d323de3-a864-4afe-a0c3-e7ff45883d7a",
      "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a"
    },
    "4": {
      "name": "get_an_alert",
      "type": "action",
      "outputs": {
        "default": [
          "1"
        ]
      },
      "arguments": {
        "stix": true,
        "uuid": "{{ node.0['alert_uuid'] }}"
      },
      "action_uuid": "8d189665-5401-4098-8d60-944de9a6199a",
      "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a"
    },
    "5": {
      "name": "post_a_comment_on_an_alert",
      "type": "action",
      "outputs": {
        "default": []
      },
      "arguments": {
        "uuid": "{{ node.0['alert_uuid'] }}",
        "author": "IP Check",
        "content": "This IP `{{ node.1['output'][0] }}` is not know to be a BitTorrent node."
      },
      "action_uuid": "0d323de3-a864-4afe-a0c3-e7ff45883d7a",
      "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a"
    }
  },
  "workspace": "Operation Center",
  "description": "Enrich with IKnowWhatYourDownload to check if the IP is known from this service, directly from SEKOIA.IO."
}
