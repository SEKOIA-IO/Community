{
    "name": "[Training usecase] Scan for url on VirusTotal",
    "description": "Enrich to check if this url.domain is known from VirusTotal, directly from Sekoia.io",
    "tags": ["alerts", "enrichment"],
    "nodes": {
      "0": {
        "icon": "data:image/svg+xml;base64,",
        "name": "Manual Trigger",
        "type": "trigger",
        "outputs": {
          "default": [
            "1"
          ]
        },
        "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a",
        "trigger_uuid": "fc26eb9f-b272-4c15-b3bf-ace397c0dc57",
      },
      "1": {
        "icon": "data:image/svg+xml;base64,",
        "name": "Get Alert",
        "type": "action",
        "outputs": {
          "default": [
            "2"
          ]
        },
        "arguments": {
          "stix": false,
          "uuid": "{{ node.0.alert_uuid }}"
        },
        "action_uuid": "8d189665-5401-4098-8d60-944de9a6199a",
        "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a",
      },
      "2": {
        "icon": "data:image/svg+xml;base64,",
        "name": "Get Events",
        "type": "action",
        "outputs": {
          "default": [
            "3"
          ]
        },
        "arguments": {
          "query": "alert_short_ids: \"{{ node.1.short_id }}\"",
          "latest_time": "{{ node.1.last_seen_at }}",
          "earliest_time": "{{ node.1.first_seen_at }}"
        },
        "action_uuid": "af0b4355-a428-43d6-991c-d5ff878e17d5",
        "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a",
      },
      "3": {
        "icon": "data:image/svg+xml;base64,",
        "name": "Read JSON File",
        "type": "action",
        "outputs": {
          "default": [
            "6"
          ]
        },
        "arguments": {
          "file": "{{ node.2| jsonpath(\"$.events[0]\", True) }}",
          "to_file": false,
          "jsonpath": "$..[\"url.domain\"]",
          "return_list": false
        },
        "action_uuid": "ecedfa52-7033-4ea8-9c3b-93ad34295f87",
        "module_uuid": "07cce76b-a319-40ee-a0cf-1ba433431e21"
      },
      "5": {
        "icon": "data:image/svg+xml;base64,",
        "name": "Comment Alert",
        "type": "action",
        "outputs": {
          "default": []
        },
        "arguments": {
          "uuid": "{{ node.0.alert_uuid }}",
          "content": "Number of positives results:{{ node.6.positives }} for {{ node.6.total }} scans in total"
        },
        "action_uuid": "0d323de3-a864-4afe-a0c3-e7ff45883d7a",
        "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a",
      },
      "6": {
        "icon": "data:image/svg+xml;base64,",
        "name": "Scan URL",
        "type": "action",
        "outputs": {
          "detected": [
            "5"
          ],
          "not detected": [
            "5"
          ],
          "unknown": [
            "5"
          ]
        },
        "arguments": {
          "url": "{{ node.3.output }}"
        },
        "action_uuid": "b43a7897-b92c-4fe1-8a5c-be9e023c4f1c",
        "module_uuid": "d023af1d-25d8-465b-b85f-2ed48214d6a5",
      }
    },
    "workspace": "Operation Center"
}