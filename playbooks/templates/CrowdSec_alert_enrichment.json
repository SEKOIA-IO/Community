{
    "name": "Enrich with CrowdSec Smoke database",
    "nodes": {
        "0": {
            "name": "Manual Trigger",
            "type": "trigger",
            "outputs": {
                "default": [
                    "4"
                ]
            },
            "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a",
            "trigger_uuid": "fc26eb9f-b272-4c15-b3bf-ace397c0dc57"
        },
        "3": {
            "name": "Post a comment to the Alert",
            "type": "action",
            "outputs": {
                "default": []
            },
            "arguments": {
                "uuid": "{{ node.0['alert_uuid'] }}",
                "author": "CrowdSec Smoke DB Playbook",
                "content": "{{ node.6.default.value }}"
            },
            "action_uuid": "0d323de3-a864-4afe-a0c3-e7ff45883d7a",
            "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a"
        },
        "4": {
            "name": "Get Alert",
            "type": "action",
            "outputs": {
                "default": [
                    "10"
                ]
            },
            "arguments": {
                "stix": true,
                "uuid": "{{ node.0['alert_uuid'] }}"
            },
            "action_uuid": "8d189665-5401-4098-8d60-944de9a6199a",
            "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a"
        },
        "5": {
            "icon": "data:image/svg+xml;base64,",
            "name": "Request URL",
            "type": "action",
            "outputs": {
                "default": [
                    "8"
                ]
            },
            "arguments": {
                "url": "https://cti.api.crowdsec.net/v1/smoke/{{ node.10.events[0][\"source.ip\"] }}",
                "method": "get",
                "headers": {
                    "x-api-key": "YOUR API KEY HERE"
                }
            },
            "action_uuid": "40bcf3c0-aa8b-4111-9b4e-f3caffccb4e5",
            "module_uuid": "5894985f-91eb-46db-9306-cc5ac6463d3d"
        },
        "6": {
            "loop": [
                "3"
            ],
            "name": "Foreach",
            "type": "operator",
            "items": "{{ store.json| jsonpath(\"$.attack_categories..description\", True) }}",
            "outputs": {
                "default": []
            },
            "subtype": "foreach"
        },
        "7": {
            "name": "Store",
            "type": "operator",
            "outputs": {
                "default": [
                    "6"
                ]
            },
            "subtype": "store",
            "modifications": [
                {
                    "key": "json",
                    "type": "set",
                    "value": "{{ node.5.json }}"
                }
            ]
        },
        "8": {
            "name": "200 OK or 404 Not Found",
            "type": "operator",
            "cases": [
                {
                    "left": "{{ node.5.reason }}",
                    "name": "Not Found",
                    "right": "Not Found",
                    "comparison": "==",
                    "description": "404 Not Found"
                },
                {
                    "left": "{{ node.5.reason }}",
                    "name": "OK",
                    "right": "OK",
                    "comparison": "==",
                    "description": "200 OK"
                }
            ],
            "outputs": {
                "OK": [
                    "7"
                ],
                "else": [],
                "Not Found": [
                    "9"
                ]
            },
            "subtype": "condition"
        },
        "9": {
            "name": "Comment Alert",
            "type": "action",
            "outputs": {
                "default": []
            },
            "arguments": {
                "uuid": "{{ node.0['alert_uuid'] }}",
                "author": "Crowdsec Smoke DB Playbook",
                "content": "No information on the IP address {{ node.1['output'][0] }} on CrowdSec"
            },
            "action_uuid": "0d323de3-a864-4afe-a0c3-e7ff45883d7a",
            "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a"
        },
        "10": {
            "name": "Get Events",
            "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a",
            "outputs": {
                "default": [
                    "5"
                ]
            },
            "type": "action",
            "action_uuid": "af0b4355-a428-43d6-991c-d5ff878e17d5",
            "arguments": {
                "query": "alert_short_ids: \"{{ node.4.short_id }}\"",
                "latest_time": "\"{{ node.4.last_seen_at }}\"",
                "earliest_time": "\"{{ node.4.first_seen_at }}\""
            }
        }
    },
    "workspace": "Operation Center",
    "description": "Enrich with CrowdSec Smoke DB to check if the IP is known from this service direclty from SEKOIA.IO.",
    "tags": [
        "CrowdSec",
        "alerts",
        "enrichment"
    ],
