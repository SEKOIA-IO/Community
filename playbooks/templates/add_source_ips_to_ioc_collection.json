{
  "name": "Add Source IP address to IOC Collection",
  "tags": [
    "alerts",
    "IOC Collection",
    "blocklist"
  ],
  "workspace": "Operation Center",
  "description": "Add the source ip addresses from the events linked to the alert to an IOC collection",
  "nodes": {
    "0": {
      "icon": "data:image/svg+xml;base64,",
      "name": "Manual Trigger",
      "type": "trigger",
      "outputs": {
        "default": [
          "1"
        ]
      },
      "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a",
      "trigger_uuid": "fc26eb9f-b272-4c15-b3bf-ace397c0dc57"
    },
    "1": {
      "icon": "data:image/svg+xml;base64,",
      "name": "Get Alert",
      "type": "action",
      "outputs": {
        "default": [
          "2"
        ]
      },
      "arguments": {
        "stix": false,
        "uuid": "{{ node.0.alert_uuid }}"
      },
      "action_uuid": "8d189665-5401-4098-8d60-944de9a6199a",
      "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a"
    },
    "2": {
      "icon": "data:image/svg+xml;base64,",
      "name": "Get source IP addresses",
      "type": "action",
      "outputs": {
        "default": [
          "3"
        ]
      },
      "arguments": {
        "query": "{{ node.1.short_id }}",
        "fields": "source.ip",
        "latest_time": "{{ node.1.last_seen_at }}",
        "earliest_time": "{{ node.1.first_seen_at }}"
      },
      "action_uuid": "e26662f6-c40d-45ae-bd3f-4f1a13684ffb",
      "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a"
    },
    "3": {
      "name": "Source IP addresses found ?",
      "type": "operator",
      "cases": [
        {
          "left": "{{ node.2| jsonpath(\"$.fields[*].common_values[*]['name']\", True) | length }}",
          "name": "Source IP addresses in Alert",
          "right": "1",
          "comparison": ">="
        }
      ],
      "outputs": {
        "else": [
          "6"
        ],
        "Source IP addresses in Alert": [
          "4"
        ]
      },
      "subtype": "condition"
    },
    "4": {
      "icon": "data:image/svg+xml;base64,",
      "name": "Add IP to IOC Collection",
      "type": "action",
      "outputs": {
        "default": [
          "5"
        ]
      },
      "arguments": {
        "indicators": "{{ node.2| jsonpath(\"$.fields[*].common_values[*]['name']\", True) }}",
        "indicator_type": "IP address",
        "ioc_collection_id": "ioc-collection--xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"
      },
      "action_uuid": "0acabc04-43b4-4564-b9b2-08b80e0e1ecf",
      "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a"
    },
    "5": {
      "icon": "data:image/svg+xml;base64,",
      "name": "Comment Success",
      "type": "action",
      "outputs": {
        "default": []
      },
      "arguments": {
        "uuid": "{{ node.0.alert_uuid }}",
        "content": "{{ node.2| jsonpath(\"$.fields[*].common_values[*]['name']\", True) | length }} source IP address(es) were added to the IOC Collection"
      },
      "action_uuid": "0d323de3-a864-4afe-a0c3-e7ff45883d7a",
      "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a"
    },
    "6": {
      "icon": "data:image/svg+xml;base64,",
      "name": "Comment Error",
      "type": "action",
      "outputs": {
        "default": []
      },
      "arguments": {
        "uuid": "{{ node.0.alert_uuid }}",
        "content": "Error - no source IP addresses were found in the Alert"
      },
      "action_uuid": "0d323de3-a864-4afe-a0c3-e7ff45883d7a",
      "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a"
    }
  }
}