{
    "uuid": "9743354f-39bc-4172-b570-a75c9ce55a9b",
    "name": "[Training usecase] Post an HTTP request based on alert information",
    "description": "Send HTTP request to an external service based on alert data",
    "tags": [
        "alerts",
        "webhook"
    ],
    "nodes": {
        "0": {
            "icon": "data:image/svg+xml;base64,",
            "name": "Manual Trigger",
            "type": "trigger",
            "outputs": {
                "default": [
                    "1"
                ]
            },
            "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a",
            "trigger_uuid": "fc26eb9f-b272-4c15-b3bf-ace397c0dc57"
        },
        "1": {
            "icon": "data:image/svg+xml;base64,",
            "name": "Get Alert",
            "type": "action",
            "outputs": {
                "default": [
                    "2"
                ]
            },
            "arguments": {
                "stix": false,
                "uuid": "{{ node.0.alert_uuid }}"
            },
            "action_uuid": "8d189665-5401-4098-8d60-944de9a6199a",
            "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a"
        },
        "2": {
            "icon": "data:image/svg+xml;base64,",
            "name": "Get Events",
            "type": "action",
            "outputs": {
                "default": [
                    "3"
                ]
            },
            "arguments": {
                "query": "alert_short_ids:\"{{ node.1.short_id }}\"",
                "latest_time": "{{ node.1.last_seen_at }}",
                "earliest_time": "{{ node.1.first_seen_at }}"
            },
            "action_uuid": "af0b4355-a428-43d6-991c-d5ff878e17d5",
            "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a"
        },
        "3": {
            "icon": "data:image/svg+xml;base64,",
            "name": "Request URL",
            "type": "action",
            "outputs": {
                "default": []
            },
            "arguments": {
                "url": "https://webhook.site/5859f51c-a91c-4abe-ac78-e9c60a0b2ef4",
                "json": "Event with the message {{ node.2| jsonpath(\"$.events[0]['message']\", True) }} raised the alert {{ node.1.rule['name'] }}",
                "method": "post",
                "verify_ssl": true,
                "fail_on_http_error": true
            },
            "action_uuid": "40bcf3c0-aa8b-4111-9b4e-f3caffccb4e5",
            "module_uuid": "5894985f-91eb-46db-9306-cc5ac6463d3d"
        }
    },
    "workspace": "Operation Center"
}