{
    "name": "HarfangLab telemetry events",
    "tags": [
        "alerts",
        "enrich",
        "events"
    ],
    "nodes": {
      "1": {
        "icon": "data:image/png;base64,",
        "name": "Alert Created",
        "type": "trigger",
        "outputs": {
          "default": [
            "3"
          ]
        },
        "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a",
        "trigger_uuid": "785583cf-4377-43ce-a5fb-f4572eb57456"
      },
      "2": {
        "icon": "data:image/png;base64,",
        "name": "Get agent telemetry",
        "type": "action",
        "outputs": {
          "default": [
            "5"
          ]
        },
        "arguments": {
          "agent_id": "{{ node.4.default.value }}",
          "timerange": 15,
          "alert_created": "{{ node.1.first_seen_at }}"
        },
        "action_uuid": "c95cf5c0-8d5c-425d-907f-09c9c2bb7edb",
        "module_uuid": "8380240b-61a4-48b7-93e4-044a7ee2309b"
      },
      "3": {
        "icon": "data:image/png;base64,",
        "name": "Get Events",
        "type": "action",
        "outputs": {
          "default": [
            "4"
          ]
        },
        "arguments": {
          "query": "alert_short_ids: {{ node.1.short_id }}",
          "latest_time": "{{ node.1.last_seen_at }}",
          "earliest_time": "{{ node.1.first_seen_at }}"
        },
        "action_uuid": "af0b4355-a428-43d6-991c-d5ff878e17d5",
        "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a"
      },
      "4": {
        "loop": [
          "2"
        ],
        "name": "Foreach agent",
        "type": "operator",
        "items": "{{ node.3| jsonpath(\"$.events[?(@['sekoiaio.intake.dialect']=='harfanglab edr')]['agent.id']\", True) }}",
        "outputs": {
          "default": []
        },
        "subtype": "foreach"
      },
      "5": {
        "loop": [
          "6"
        ],
        "name": "Foreach",
        "type": "operator",
        "items": "{{ node.2| jsonpath(\"$.data[*]\", True) }}",
        "outputs": {
          "default": []
        },
        "subtype": "foreach"
      },
      "6": {
        "icon": "data:image/png;base64,",
        "name": "Push Events to Intake",
        "type": "action",
        "outputs": {
          "default": []
        },
        "arguments": {
          "event": "{{ node.5.default.value }}",
          "intake_key": "",
          "intake_server": "https://intake.sekoia.io",
          "keep_file_after_push": false
        },
        "action_uuid": "10314466-ac0c-4067-8e81-4093e3d1e1da",
        "module_uuid": "92d8bb47-7c51-445d-81de-ae04edbb6f0a"
      }
    },
    "description": "Retrieve additional telemetry when an HarfangLab alert is raised",
    "workspace": "Operation Center",
    "uuid": "5c266286-1bdf-47a5-af4c-f01b1c0e4631"
  }